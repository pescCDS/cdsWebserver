            <xs:enumeration value="SignLanguageReader"/>
            <xs:enumeration value="SpecialLighting"/>
            <xs:enumeration value="SpecialFurniture"/>
            <xs:enumeration value="Trackball"/>
            <xs:enumeration value="Typewriter"/>
            <xs:enumeration value="Untimed"/>
            <xs:enumeration value="Unspecfied"/>
            <xs:enumeration value="UnusualTestingEvent"/>
            <xs:enumeration value="VideotapeScripts"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RawScoreValueType">
        <xs:annotation>
            <xs:documentation> The weighted number of correct answers</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="PercentCorrectValueType">
        <xs:annotation>
            <xs:documentation> The number of correct answers divided by the number of possible correct answers</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0"/>
            <xs:maxInclusive value="100.0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ScaleScoreValueType">
        <xs:annotation>
            <xs:documentation> A test result that is transformed so that the result is equated with previous forms of the test</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="LabelValueType">
        <xs:annotation>
            <xs:documentation> A test result that is a name instead of a number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LocalValueType">
        <xs:annotation>
            <xs:documentation> A test result that is defined by the test reporting organization</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="MasteredIndicatorType">
        <xs:annotation>
            <xs:documentation> Indicator that the student has met the standard of the test</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean"/>
    </xs:simpleType>
    <xs:simpleType name="CutScoreType">
        <xs:annotation>
            <xs:documentation> The value used to determine that the subject has been mastered</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal"/>
    </xs:simpleType>
    <xs:simpleType name="NormPopulationCodeType">
        <xs:annotation>
            <xs:documentation> The group of individuals to which the student's score is compared</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Institution"/>
            <xs:enumeration value="City"/>
            <xs:enumeration value="County"/>
            <xs:enumeration value="State"/>
            <xs:enumeration value="Region"/>
            <xs:enumeration value="National"/>
            <xs:enumeration value="International"/>
            <xs:enumeration value="Other"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NormPopulationDescriptionType">
        <xs:annotation>
            <xs:documentation> Text specifying the exact nature of the norm referenced population</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NormPopulationSizeType">
        <xs:annotation>
            <xs:documentation> The number of individuals that c.prise the norm referenced population</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedInt"/>
    </xs:simpleType>
    <xs:simpleType name="RankValueType">
        <xs:annotation>
            <xs:documentation> The ordinal position of the student's score relative to other students in the norm population.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:unsignedLong"/>
    </xs:simpleType>
    <xs:simpleType name="PercentileLowerBoundValueType">
        <xs:annotation>
            <xs:documentation> The percentage of norm population that scored below a specific score</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0"/>
            <xs:maxInclusive value="100.0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PercentileRankValueType">
        <xs:annotation>
            <xs:documentation> The percentage of the norm population that scored below the median of a specific score</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0"/>
            <xs:maxInclusive value="100.0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PercentileUpperBoundValueType">
        <xs:annotation>
            <xs:documentation> The percentage of the norm group that scored at or below a specific score.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0.0"/>
            <xs:maxInclusive value="100.0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StandardScoreValueType">
        <xs:annotation>
            <xs:documentation> Scores are transformed to a z statistic with a mean of zero and a standard deviation of 1</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="-10.0"/>
            <xs:maxInclusive value="+10.0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NormalCurveValueType">
        <xs:annotation>
            <xs:documentation> Scores are transformed so that they approximate the Gaussian distribution</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal"/>
    </xs:simpleType>
    <xs:simpleType name="StanineValueType">
        <xs:annotation>
            <xs:documentation> Scores are transfomed to a integer scale of 1 to 9 with a mean of 5 and a standard deviation of 2</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="9"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProbabilityValueType">
        <xs:annotation>
