        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Qualifying"/>
            <xs:enumeration value="OralComprehensive"/>
            <xs:enumeration value="WrittenComprehensive"/>
            <xs:enumeration value="Candidacy"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
