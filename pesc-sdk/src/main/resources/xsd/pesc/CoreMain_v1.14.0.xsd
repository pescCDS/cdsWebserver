    <!--AcademicDegreeRequirement Types-->
    <!--=========================================-->
    <xs:simpleType name="OralDefenseCompletedIndicatorType">
        <xs:annotation>
            <xs:documentation>An indication of the student's completion of an oral defense. The requirement to conduct an oral defense by doctoral students may vary across institutions, programs, or fields of study. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean"/>
    </xs:simpleType>
    <xs:simpleType name="OralDefenseDateType">
        <xs:annotation>
