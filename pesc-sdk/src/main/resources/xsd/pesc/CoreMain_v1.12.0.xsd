			<xs:enumeration value="PermanentVisa"/>
			<xs:enumeration value="Refugee"/>
			<xs:enumeration value="ResidentAlien"/>
			<xs:enumeration value="StudentAuthorization"/>
			<xs:enumeration value="StudentVisa"/>
			<xs:enumeration value="TemporaryVisa"/>
			<xs:enumeration value="UndocumentedAlien"/>
