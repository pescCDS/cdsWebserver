	</xs:simpleType>
	<!--=====================================-->
	<!--Loan Information related simpleType's-->
	<!--=====================================-->
	<xs:simpleType name="BlanketGuaranteeIndicatorType">
		<xs:annotation>
			<xs:documentation>Indicates if submission is a blanket guarantee</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="BlanketGuaranteeDateType">
