<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="James Whetstone" context="dev, ci, prod">
        <comment>Adding the 'JSON' document format</comment>

        <insert tableName="document_format">
            <column name="name" value="JSON"/>
            <column name="description" value="JavaScript Object Notation"/>
        </insert>

    </changeSet>
</databaseChangeLog>