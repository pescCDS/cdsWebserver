<?xml version="1.0" encoding="utf-8"?>
<bindings  version="2.1"
             xmlns="http://java.sun.com/xml/ns/jaxb"
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
         xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
       xmlns:annox="http://annox.dev.java.net"
        xmlns:EdEx="urn:org:pesc:EdExchange:v1.0.8"
xsi:schemaLocation="EdExchangeEnvelope_v1.0.8.xsd 
					http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">
    
    <jaxb:bindings schemaLocation="EdExchangeEnvelope_v1.0.8.xsd">
    	
    	<jaxb:globalBindings>
    		
    		<jaxb:javaType name="java.util.Calendar" xmlType="xs:date"
				parseMethod="javax.xml.bind.DatatypeConverter.parseDate" 
				printMethod="javax.xml.bind.DatatypeConverter.printDate" />
    		
    		<jaxb:javaType name="java.util.Calendar" xmlType="xs:dateTime"
				parseMethod="org.pesc.cds.xml.adapter.XMLAdapter.parseDateTime" 
				printMethod="org.pesc.cds.xml.adapter.XMLAdapter.printDateTime" />
	        
	        <jaxb:javaType name="java.sql.Timestamp" xmlType="EdEx:Timestamp" 
	        	printMethod="org.pesc.cds.xml.adapter.XMLAdapter.printTimestamp"
	        	parseMethod="org.pesc.cds.xml.adapter.XMLAdapter.parseTimestamp" />
	        
	        <jaxb:javaType name="java.lang.Integer" xmlType="xs:positiveInteger"
	        	parseMethod="org.pesc.cds.xml.adapter.XMLAdapter.parseInteger" />
	        
	        <jaxb:javaType name="java.lang.Integer" xmlType="xs:boolean" 
				parseMethod="org.pesc.cds.xml.adapter.XMLAdapter.parseBoolean"
				printMethod="org.pesc.cds.xml.adapter.XMLAdapter.printBoolean" />
	        
    	</jaxb:globalBindings>
    	
	</jaxb:bindings>
	
</bindings>