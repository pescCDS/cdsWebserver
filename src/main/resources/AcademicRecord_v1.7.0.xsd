			<xs:element name="Person" type="AcRec:K12PersonType"/>
			<xs:element name="AcademicRecord" type="AcRec:AcademicRecordType" maxOccurs="unbounded"/>
			<xs:element name="Health" type="core:HealthType" minOccurs="0"/>
